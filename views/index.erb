  <body>

    <div class="container">
      <div class="jumbotron">
        <h1>Some Dumb Space Game</h1>
        <p class="lead">It's hard to not shoot yourself, I guess.</p>
        <p><a class="btn btn-lg btn-success disabled" href="#" role="button">Download</a></p>
      </div>

      <div class="row marketing">
        <div class="col-sm-6 col-sm-offset-3">
          <h1>High Scores!</h1>
          <ul class="list-group">
            <% scores.each do |score| %>
              <li class="list-group-item">
                <span class="badge"><%= score.score %></span>
                <%= score.name %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>

      <footer class="footer">
        <p>&copy; 2016 Company, Inc.</p>
      </footer>

    </div> <!-- /container -->


    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
  </body>
